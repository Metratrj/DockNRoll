{
  "name": "metratrj/dock-n-roll",
  "description": "Minimal Docker Management Tool in PHP",
  "repositories": [{
    "type": "path",
    "url": "app/External/docker-php-client",
    "options": {
      "symlink": true
    }
  }
  ],
  "require": {
    "php": "^8.2",
    "guzzlehttp/guzzle": "^7.10",
    "guzzlehttp/psr7": "^2.8",
    "metratrj/docker-php-client": "1.0.0"
  },
  "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.87"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "DockerClient\\": "app/External/docker-php-client/lib/"
    }
  },
  "scripts": {
    "cs": "phpcs",
    "cbf": "phpcbf"
  }
}
